{
  "extends": [
    "eslint-config-egg",
    "eslint-config-egg/typescript",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/ban-types": "off",
    "import/no-relative-packages": "error",
    "max-len": [
      "error",
      {
        "code": 80
      }
    ]
  }
}
